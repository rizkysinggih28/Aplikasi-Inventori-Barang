<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KategoriActivity">

    <TextView
        android:id="@+id/TV_Back_Kategori"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="25dp"
        android:drawableStart="@drawable/ic_arrow_back_white_30dp"/>

    <TextView
        android:id="@+id/TambahKategori"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="345dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="15dp"
        android:drawableStart="@drawable/ic_add_circle_black_50dp"/>
</RelativeLayout>